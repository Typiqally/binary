{
    "name": "typiqally/binary",
    "description": "PHP Binary Serializer",
    "keywords": [
        "typiqally",
        "binary",
        "packet",
        "serialization",
        "serializer",
        "deserialization",
        "deserializer"
    ],
    "homepage": "https://github.com/Typiqally/binary",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Jelle Maas",
            "email": "typiqally@gmail.com",
            "homepage": "https://typiqally.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Typiqally\\Binary\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Typiqally\\Binary\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "support": {
        "issues": "https://github.com/Typiqally/binary/issues",
        "source": "https://github.com/Typiqally/binary"
    }
}
